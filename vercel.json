{
    "version": 2,
    "builds": [
      {
        "src": "*.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/profile",
        "dest": "/profile.html"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "buildCommand": false,
    "devCommand": false,
    "installCommand": false
  }